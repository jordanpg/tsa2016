        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Handle input logic

spd = 1;
anm_spd = .18;

if(keyboard_check(vk_anykey)){
    
    //movement
    if(keyboard_check(ord("W"))){
        sprite_index = Spr_GK_Up;
        phy_position_y -= spd;
        image_speed = anm_spd;
    }
    if(keyboard_check(ord("S"))){
        sprite_index = Spr_GK_Dwn;
        phy_position_y += spd;
        image_speed = anm_spd;
    }
    if(keyboard_check(ord("D"))){
        sprite_index = Spr_GK_Rgt;
        phy_position_x += spd;
        image_speed = anm_spd;
    }
    if(keyboard_check(ord("A"))){
        sprite_index = Spr_GK_Lft;
        phy_position_x -= spd;
        image_speed = anm_spd;
    }
    
    //npc interaction
    if(keyboard_check(ord("T"))){
        //interaction
        action = true;
    }
    
} else {
    //set character speed to 0 sense character isnt moving
    image_speed = 0;
    
    //set action to flase since interaction key isnt pressed anymore
    action = false;
}
</string>
